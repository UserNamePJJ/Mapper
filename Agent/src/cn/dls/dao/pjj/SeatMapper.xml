<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.dls.dao.pjj.SeatMapper">
<select id="findall" resultMap="list">
SELECT s.`seatid`, s.`seatname`, t.id,t.typename FROM `seat` s,transactiontype t where s.transactionid=t.id
</select>
<resultMap type="Seat" id="list">
<id property="seatid" column="seatid"/>
<result property="seatname" column="seatname"/>
<association property="trans" javaType="Transactiontype">
<id property="id" column="id"/>
<result property="typename" column="typename"/>
</association>
</resultMap>
</mapper>